<% include ./partials/head %>
<!-- Basic right 75% structure has for div's for each section to render -->
<body>
   <div class="page-header">
       <div class="container">
                <div class="navbar-header">
                    <a href="http://www.microsoft.com" class="navbar-brand">
                        <img src="https://assets.onestore.ms/cdnfiles/onestorerolling-1511-11008/shell/v3/images/logo/microsoft.png" alt="Microsoft" height="23">
                    </a>
                </div>
       </div>
        <div class="navbar-local color-accent theme-dark">
            <div class="container">
                <div class="row">
                <div class="col-xs-24">
                    <h2 style="color:white;"><%= welcomeMessage %></h2>
                    <p></p>
                    
                </div>
            </div>
        </div>
    </div>
   </div>
<div class="container">
<div class="row">
                <div class="col-xs-24">              
                  <section class="section">
                      <div class="row m-t-md">
                        <% include ./partials/leftnavigation %>
                        <div class="col-xs-18 col-md-18 navigation-section">

                            <div class="col-xs-24 col-md-24 navigation-section">
                                <a name="introduction"></a>
                                <p><%- include(content.introduction) %></p>
                            </div>
                            <div class="col-xs-24 col-md-24 navigation-section">
                                <%- content.presentations %>
                            </div>
                            <div class="col-xs-24 col-md-24 navigation-section">
                                <%- content.videos %>
                            </div>
                            <div class="col-xs-24 col-md-24 navigation-section">
                                <%- content.assignments %>
                            </div>

                        </div>
                      </div>                        
                    </div>
                </section>
            </section>
        </div>
    </div>
   </div>
   


</body>
</html>
