<html>
<head>
    <link href="/scripts/video.js/dist/video-js.css" rel="stylesheet">
    <link href="/scripts/videojs-chapter-thumbnails/dist/videojs.chapter-thumbnails.min.css" rel="stylesheet">
    <script src="/js/jquery-1.12.1.js"></script>
    <script src='/scripts/video.js/dist/video.min.js'></script>
    <script src='/scripts/videojs-chapter-thumbnails/dist/videojs.chapter-thumbnails.js'></script>
</head>
<body>
<h1>Here be breaded cat videos</h1>
<ul>
    <% for(var i=0; i < data.length; i++) {%>
        <li>
            <video id='<%=data[i]%>' class='video-js vjs-default-skin' controls preload='auto' width='300' height="124"> <source src='/files/video/<%=data[i]%>.mp4'> type="video/mp4" /></video>
        </li> 
    <% } %>
</ul>
<script>
        $('document').ready(function(){
            $('video').each(function(){
                var id = $(this).attr("id")
                videojs($(this).attr("id")).ready(function(){this.chapter_thumbnails({
            src: '/files/video/' + id + ".vtt"
                })
            })
        });
    }); 
</script>
</body>
</html>
